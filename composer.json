    "homepage": "http://www.helpscout.net",
    "license": "MIT",
    "authors": [
        {
            "name": "Help Scout",
            "email": "developer@helpscout.net",
            "homepage": "http://developer.helpscout.net/",
            "role": "Developer"
        },
        {
            "name": "m1x0n",
            "email": "mmorozovm@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "mmorozovm@gmail.com",
        "source": "https://github.com/m1x0n/helpscout-docs-api-php"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "HelpScoutDocs\\": "src/HelpScoutDocs/"
        }
    },
